{
  "tauri": {
    "package": {
      "productName": "Fibonacci Calculator",
      "version": "0.1.0",
      "description": "A desktop application to calculate Fibonacci numbers.",
      "author": "Your Name",
      "license": "MIT"
    },
    "build": {
      "beforeBuildCommand": "",
      "beforeDevCommand": "",
      "distDir": "../dist",
      "devPath": "../src/index.html",
      "withGlobalTauri": false
    },
    "tauri": {
      "window": {
        "title": "Fibonacci Calculator",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true
        
      },
      "systemTray": {
        "iconPath": "icons/tray_icon.png"
      },
      "security": {
        "csp": "default-src 'self'; img-src 'self' data:; script-src 'self'; style-src 'self' 'unsafe-inline';"

      }
    }
  }
}